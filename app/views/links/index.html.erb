<div class="row">
    <div class="col s3 card-panel">
      <h2>Make Another Link</h2>
      <div id="links-index-form">
        <%= render 'links_index_form' =%>
      </div>
    </div>
  <div class="col s9">
    <h2>Your Links</h2>
    <button data-target="share_links_modal" class="btn modal-trigger waves-effect waves-light light-blue darken-4">Share Your Links</button>
    <div>
      <%= render 'share_links_modal' =%>
    </div>
    <div>
        <%= render 'links_table' =%>
    </div>
  </div>
</div>
